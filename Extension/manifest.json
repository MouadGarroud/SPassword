{
  "manifest_version": 3,
  "name": "SPassword",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
"action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
